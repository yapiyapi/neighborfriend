<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="32dp"
    tools:context=".Activity_Find_id_get">



    <ImageView
        android:id="@+id/back_btn_find_id_get"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:src="@drawable/back_button"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/title_find_id_get"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="80dp"
        android:text="아이디 찾기"
        android:textSize="24sp"
        app:layout_constraintStart_toEndOf="@+id/back_btn_find_id_get"
        app:layout_constraintTop_toTopOf="@+id/back_btn_find_id_get" />

    <TextView
        android:id="@+id/textView_find_id_get"
        android:layout_width="347dp"
        android:layout_height="305dp"
        android:layout_marginTop="100dp"
        android:ems="10"
        android:text="아이디는\n\n입니다."
        android:textColor="@color/black"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/back_btn_find_id_get" />

    <TextView
        android:id="@+id/textview_to_login"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="로그인 페이지로"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>